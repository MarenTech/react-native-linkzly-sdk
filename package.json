{
  "name": "@linkzly/react-native-sdk",
  "version": "1.0.2",
  "description": "Linkzly React Native SDK for deep linking and attribution tracking",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "lib/module/index.js",
  "source": "src/index.tsx",
  "files": [
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build",
    "prepack": "bob build",
    "prepublishOnly": "bob build",
    "release": "release-it",
    "bootstrap": "yarn install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "linkzly",
    "deep-linking",
    "attribution",
    "mobile-attribution",
    "universal-links",
    "app-links"
  ],
  "repository": "https://github.com/MarenTech/react-native-linkzly-sdk.git",
  "author": "Linkzly <support@linkzly.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarenTech/react-native-linkzly-sdk/issues"
  },
  "homepage": "https://github.com/MarenTech/react-native-linkzly-sdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "@types/react-native": "^0.72.8",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-native": ">=0.71.2"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
